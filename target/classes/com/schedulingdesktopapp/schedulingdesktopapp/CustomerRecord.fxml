<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="500.0" maxWidth="1100.0" minHeight="500.0" minWidth="1100.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.schedulingdesktopapp.controller.CustomerRecordController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
      <HBox prefHeight="29.0" prefWidth="860.0">
         <children>
            <Label text="Customers">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#onActionClickAdd" prefHeight="25.0" prefWidth="78.0" text="Add" translateX="720.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button layoutY="10.0" mnemonicParsing="false" onAction="#onActionClickUpdate" prefHeight="25.0" prefWidth="78.0" text="Update" translateX="730.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
            <Button layoutY="10.0" mnemonicParsing="false" onAction="#onActionClickDelete" prefHeight="25.0" prefWidth="78.0" text="Delete" translateX="740.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="400.0" prefWidth="800.0">
         <children>
            <TableView fx:id="tableView" prefHeight="400.0" prefWidth="1100.0">
              <columns>
                <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="name" prefWidth="119.0" text="Name" />
                  <TableColumn fx:id="address" prefWidth="169.0" text="Address" />
                  <TableColumn fx:id="postal" prefWidth="102.0" text="Postal Code" />
                  <TableColumn fx:id="phone" prefWidth="106.0" text="Phone" />
                  <TableColumn fx:id="createdDate" prefWidth="111.0" text="Created" />
                  <TableColumn fx:id="createdBy" prefWidth="105.0" text="Created By" />
                  <TableColumn fx:id="updatedDate" prefWidth="95.0" text="Updated" />
                  <TableColumn fx:id="updatedBy" maxWidth="300.0" prefWidth="94.0" text="Updated By" />
                  <TableColumn fx:id="divisionID" prefWidth="81.0" text="Division ID" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onActionClickMenu" prefHeight="25.0" prefWidth="140.0" text="Main Menu" />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="140.0" text="Open Record" translateX="780.0" />
         </children>
      </HBox>
   </children>
</VBox>
